            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import groovy.json.JsonOutput;
import groovy.json.JsonSlurper;
import org.apache.jmeter.services.FileServer;

class ArgParser
{
	static def parse(String[] argv)
	{
		def res = [:];

		for (arg in argv) {
			def bits = arg.split(/=/)
			res[bits[0]]=bits[1];
		}

		return res
	}
}

class StatsRunner
{
    String workDir

    def StatsRunner(String workDir)
    {
        this.workDir = workDir;
    }

    def Map run()
    {
        def res = &quot;docker compose stats --no-stream --format json&quot;.execute([], new File(workDir))
        res.waitFor()

        String logStr = res.text

        return [
            &apos;dt&apos;: (new Date()).format(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss.SSSZ&quot;),
            &apos;exitCode&apos;: res.exitValue(),
            &apos;data&apos;: logStr.readLines().collect({String jsonStr -&gt; (new JsonSlurper()).parseText(jsonStr)})
        ]
    }
}

def argsMap = ArgParser.parse(args)

def rootDir = FileServer.getFileServer().getBaseDir();
def runner = new StatsRunner(rootDir);
res = runner.run();
SampleResult.setResponseHeaders(&quot;X-Root-Dir: ${rootDir}&quot;)
SampleResult.setResponseData(JsonOutput.toJson(res), &quot;UTF-8&quot;)</stringProp>
